{
	"extends": "tslint:latest",
	"rules": {
		"adjacent-overload-signatures": false,
		"align": false, //TODO
		"only-arrow-functions": false, //todo: This was activating for `declare function socket(type: string, options?: any): Socket;`
		"class-name": false, //todo
		"comment-format": false, //todo
		"curly": false,
		"eofline": false,
		"indent": false,
		"interface-name": false,
		"jsdoc-format": false,
		"max-line-length": false, //todo
		"member-access": false,
		"member-ordering": false,
		"no-bitwise": false,
		"no-consecutive-blank-lines": false,
		"no-console": false,
		"no-namespace": false,
		"no-reference": false, //todo
		"no-shadowed-variable": false,
		"no-string-literal": false,
		"no-unused-variable": false,
		"no-var-keyword": false, //todo
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"quotemark": false, //todo
		"trailing-comma": false, //todo
		"variable-name": false,
		"whitespace": [
			"check-branch",
			"check-decl",
			"check-operator",
			"check-module",
			"check-separator",
			"check-type",
			"check-typecast"
		]
	}
}
